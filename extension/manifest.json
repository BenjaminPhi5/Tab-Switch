{
    "name": "Tab Switch",
    "version": "0.1",
    "description": "switch to any currently open tab, and set its mute status",

    "permissions": ["contextMenus","activeTab", "tabs"],

    "browser_action": {
        "default_popup": "popup/popup.html",
        "default_icon": "icons/128.png",
        "persistent":"true"
    },

    "icons": {
        "16": "icons/16.png",
        "32": "icons/32.png",
		"48": "icons/48.png",
		"128": "icons/128.png",
		"256": "icons/256.png"
	},

    "manifest_version": 2

}